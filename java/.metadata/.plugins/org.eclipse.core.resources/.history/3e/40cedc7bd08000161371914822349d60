		return lecteur;
	}

	public void setLecteur(String lecteur) {
		this.lecteur = lecteur;
	}

	@Override
	public List<File> call() throws Exception {
		// TODO Auto-generated method stub
		File lec = new File(lecteur);
		List<File> 
		File[] docs = lec.listFiles();
		
		return null;
	}

}
